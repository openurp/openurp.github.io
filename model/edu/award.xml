<?xml version="1.0" encoding="utf-8"?>
<report title="数据库设计说明">
  <system name="评奖评优" version="0.0.1"/>
  <db>
    <dialect>org.beangle.data.jdbc.dialect.PostgreSQLDialect
    </dialect>
    <driver>org.postgresql.Driver</driver>
    <url>jdbc:postgresql://localhost:5432/urp_sample</url>
    <schema>edu_award</schema>
    <user>postgres</user>
    <password>postgres</password>
  </db>
  <pages template="jekyll" extension=".md">
    <page name="index" />
    <page name="tables" />
    <page name="sequences" />
    <page name="images" />
    <page name="module" iterable="true"/>
  </pages>

  <modules>
    <module name="scholarship" title="奖学金" tables="scholarship_*">
      <image name="scholarship" title="奖学金" tables="scholarship_*"/>
    </module>
    <module name="stipend" title="助学金" tables="stipend_*">
      <image name="stipend" title="助学金" tables="stipend_*"/>
    </module>
    <module name="subsidy" title="困难补助" tables="subsidy_*">
      <image name="subsidy" title="困难补助" tables="subsidy_*"/>
    </module>
    <module name="honor" title="荣誉称号" tables="honor_*,std_honor_awards">
      <image name="honor" title="荣誉称号" tables="honor_*,std_honor_awards"/>
    </module>
  </modules>
</report>
